{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "resolveJsonModule": true,
    "jsxImportSource": "solid-js"
  },
  "include": ["./src"],
  "exclude": ["node_modules/"]
}

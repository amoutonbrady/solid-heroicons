{
  "name": "@amoutonbrady/solid-heroicons",
  "version": "0.2.0",
  "description": "Heroicons for Solid",
  "main": "dist/icon.js",
  "module": "dist/icon.esm.js",
  "types": "dist/icon.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "outline",
    "solid"
  ],
  "scripts": {
    "build:icons": "yarn node -r esm scripts/fetchIcons",
    "build:component": "rollup -c",
    "build:types": "tsc",
    "build": "yarn build:icons && yarn build:component && yarn build:types",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "icon",
    "heroicon",
    "tailwind",
    "solidjs"
  ],
  "author": "Alexandre Mouton-Brady <amoutonbrady@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amoutonbrady/solid-heroicons.git"
  },
  "bugs": {
    "url": "https://github.com/amoutonbrady/solid-heroicons/issues"
  },
  "homepage": "https://github.com/amoutonbrady/solid-heroicons#readme",
  "license": "ISC",
  "peerDependencies": {
    "solid-js": "^0.20.0"
  },
  "dependencies": {
    "solid-js": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/types": "^7.11.5",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-preset-solid": "^0.20.0",
    "change-case": "^4.1.1",
    "dedent": "^0.7.0",
    "degit": "^2.8.0",
    "esm": "^3.2.25",
    "fs-extra": "^9.0.1",
    "rollup": "^2.28.2",
    "typescript": "^4.0.3"
  }
}

{
  "name": "@amoutonbrady/solid-heroicons",
  "version": "0.7.0",
  "description": "Heroicons for Solid",
  "sideEffects": false,
  "solid": "./dist/source/icon.jsx",
  "main": "dist/cjs/icon.js",
  "module": "dist/esm/icon.js",
  "browser": "dist/esm/icon.js",
  "types": "dist/types/icon.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/icon.jsx",
      "import": "./dist/esm/icon.js",
      "browser": "./dist/esm/icon.js",
      "require": "./dist/cjs/icon.js",
      "node": "./dist/cjs/icon.js"
    },
    "./outline": {
      "import": "./outline/index.esm.js",
      "browser": "./outline/index.esm.js",
      "require": "./outline/index.common.js",
      "node": "./outline/index.common.js"
    },
    "./solid": {
      "import": "./solid/index.esm.js",
      "browser": "./solid/index.esm.js",
      "require": "./solid/index.common.js",
      "node": "./solid/index.common.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "outline",
    "solid"
  ],
  "scripts": {
    "build:icons": "jiti scripts/fetchIcons",
    "build:component": "rollup -c",
    "build:types": "tsc",
    "build": "pnpm build:icons && pnpm build:component && pnpm build:types",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "icon",
    "heroicon",
    "tailwind",
    "solidjs"
  ],
  "author": "Alexandre Mouton-Brady <amoutonbrady@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amoutonbrady/solid-heroicons.git"
  },
  "bugs": {
    "url": "https://github.com/amoutonbrady/solid-heroicons/issues"
  },
  "homepage": "https://github.com/amoutonbrady/solid-heroicons#readme",
  "license": "ISC",
  "peerDependencies": {
    "solid-js": "^0.24"
  },
  "dependencies": {
    "solid-js": "0.24.12"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/types": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/dedent": "^0.7.0",
    "@types/degit": "^2.8.1",
    "@types/fs-extra": "^9.0.8",
    "babel-preset-solid": "^0.24.12",
    "change-case": "^4.1.2",
    "dedent": "^0.7.0",
    "degit": "^2.8.0",
    "fs-extra": "^9.1.0",
    "jiti": "^1.6.0",
    "rollup": "^2.40.0",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.2.2"
  }
}

{
  "name": "@amoutonbrady/solid-heroicons",
  "version": "0.4.2",
  "description": "Heroicons for Solid",
  "main": "./dist/icon.common.js",
  "module": "./dist/icon.esm.js",
  "browser": "./dist/icon.module.js",
  "types": "./dist/icon.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "browser": "./dist/icon.module.js",
      "import": "./dist/icon.esm.js",
      "require": "./dist/icon.js"
    },
    "./outline": {
      "browser": "./outline/index.esm.js",
      "require": "./outline/index.common.js",
      "import": "./outline/index.esm.js"
    },
    "./solid": {
      "browser": "./solid/index.esm.js",
      "require": "./solid/index.common.js",
      "import": "./solid/index.esm.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "outline",
    "solid"
  ],
  "scripts": {
    "build:icons": "jiti scripts/fetchIcons",
    "build:component": "rollup -c",
    "build:types": "tsc",
    "build": "pnpm build:icons && pnpm build:component && pnpm build:types",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "icon",
    "heroicon",
    "tailwind",
    "solidjs"
  ],
  "author": "Alexandre Mouton-Brady <amoutonbrady@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amoutonbrady/solid-heroicons.git"
  },
  "bugs": {
    "url": "https://github.com/amoutonbrady/solid-heroicons/issues"
  },
  "homepage": "https://github.com/amoutonbrady/solid-heroicons#readme",
  "license": "ISC",
  "peerDependencies": {
    "solid-js": "^0.22.8"
  },
  "dependencies": {
    "solid-js": "^0.22.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/types": "^7.12.6",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/dedent": "^0.7.0",
    "@types/degit": "^2.8.0",
    "@types/fs-extra": "^9.0.4",
    "babel-preset-solid": "^0.22.7",
    "change-case": "^4.1.1",
    "dedent": "^0.7.0",
    "degit": "^2.8.0",
    "fs-extra": "^9.0.1",
    "jiti": "^0.1.17",
    "rollup": "^2.33.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.0.5"
  }
}
